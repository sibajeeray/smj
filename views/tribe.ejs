                                        <option value="Data Centre Management Services">Data Centre Management Services</option>
                                        <option value="Data Ecosystem And Advanced Analytics">Data Ecosystem And Advanced Analytics</option>
                                        <option value="Data Lifecycle Evolution">Data Lifecycle Evolution</option>
                                        <option value="Data Services - Customer">Data Services - Customer</option>
                                        <option value="Data Services - Group">Data Services - Group</option>
                                        <option value="Deposits & Wealth">Deposits & Wealth</option>
                                        <option value="Deposits And Wealth">Deposits And Wealth</option>
                                        <option value="Digital Customer Experience">Digital Customer Experience</option>
                                        <option value="Digital Data And Transaction Banking">Digital Data And Transaction Banking</option>
                                        <option value="Digital Sales Experience">Digital Sales Experience</option>
                                        <option value="Document And Knowledge Management">Document And Knowledge Management</option>
                                        <option value="Edison">Edison</option>
                                        <option value="Employee Experience">Employee Experience</option>
                                        <option value="Enablement">Enablement</option>
                                        <option value="Engineering Acceleration">Engineering Acceleration</option>
                                        <option value="Enterprise Apis & Integration Services">Enterprise Apis & Integration Services</option>
                                        <option value="Enterprise Compute">Enterprise Compute</option>
                                        <option value="Enterprise Continuity">Enterprise Continuity</option>
                                        <option value="Enterprise Data Platforms">Enterprise Data Platforms</option>
                                        <option value="Enterprise Enablement">Enterprise Enablement</option>
                                        <option value="Enterprise Mobility">Enterprise Mobility</option>
                                        <option value="Enterprise Mobility Tech Area">Enterprise Mobility Tech Area</option>
                                        <option value="Enterprise Risk Technology Services">Enterprise Risk Technology Services</option>
                                        <option value="Everyday Banking">Everyday Banking</option>
                                        <option value="Financial Crime & Retail Decisioning">Financial Crime & Retail Decisioning</option>
                                        <option value="Fixed Income-Derivatives-Currencies & Market Risk">Fixed Income-Derivatives-Currencies & Market Risk
