<%- include('./includes/head.ejs') %>
<link rel="stylesheet" href="/css/review.css">
</head>

<body>
    <%- include('./includes/navbar.ejs') %>
    <div class="contents" id="requirements">

        <div class="page-height background">
            <h1 class="welcome">Review Assessment</h1>
            <input type="hidden" name="_csrf" value="<%= csrfToken %>">
            <input type="hidden" name="answers" value="<%= answers %>">
            <div class="container">

                <form action="/survey/update-requirements" method="POST" class="resupdate">
                    <div class="row submit text-center">
                        <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                        <input type="hidden" name="assessmentId" value="<%- assessmentId %>" />
                        <input type="hidden" name="user" value="<%= user %>">
                        <input type="submit" id="removefirst" value="Re-do Assessment  >>" class="submit-btn">
                    </div>
                </form>
                <% if(data === 4) { %>
                <form action="/survey/result" method="POST" class="resupdate">
                    <div class="row submit text-center">
                        <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                        <input type="hidden" name="assessmentId" value="<%- assessmentId %>" />
                        <input type="hidden" name="user" value="<%= user %>">
                        <input type="submit" id="removesecond" value="View my Assessment Result  >>" class="submit-btn">
                    </div>
                </form>
                <% } %>

                <h6 class="questions text-position">
                    Section 1. Requirements
                </h6>

                <h4 class="text-position">
                    Basic
                    <hr class="rule">
                </h4>
                <!-- Requirement section Basic questions and answers goes here               -->
                <div class="row">
                    <div class="col-md-8 questionsbox">
                        <div class="row">
                            <div class="col-md-10 questions">
                                <p class="data">
                                    Do all requirements have Acceptance Criteria
                                </p>
                            </div>
                            <div class="col-md-2 questions">
                                <span><% if(answers.requirements.requirements_basic.c1_req1 === 'true') {%>Yes<%} else {%>No<%} %></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-10 questions">
                                <p class="data">
                                    Have at least 40% of Requirements been Risk Assessed (RRA) for Testing?
                                </p>
                            </div>
                            <div class="col-md-2 questions">
                                <span><% if(answers.requirements.requirements_basic.c1_req2 === 'true') {%>Yes<%} else {%>No<%} %></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-10 questions">
                                <p class="data">
                                    Is Testing included in Req Elaboration?
                                </p>
                            </div>
                            <div class="col-md-2 questions">
                                <span><% if(answers.requirements.requirements_basic.c1_req3 === 'true') {%>Yes<%} else {%>No<%} %></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-10 questions">
                                <p class="data">
                                    Is Testing included in all estimation / planning?
                                </p>
                            </div>
                            <div class="col-md-2 value">
                                <span><% if(answers.requirements.requirements_basic.c1_req4 === 'true') {%>Yes<%} else {%>No<%} %></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-10 questions">
                                <p class="data">
                                    Has DoD been agreed?
                                </p>
                            </div>
                            <div class="col-md-2 value">
                                <span><% if(answers.requirements.requirements_basic.c1_req5 === 'true') {%>Yes<%} else {%>No<%} %></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-10 questions">
                                <p class="data">
                                    Has Testing of requirements considered non-functional types?
                                </p>
                            </div>
                            <div class="col-md-2 value">
                                <span><% if(answers.requirements.requirements_basic.c1_req6 === 'true') {%>Yes<%} else {%>No<%} %></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-10 questions">
                                <p class="data">
                                    Have Non Functional requirements been defined?
                                </p>
                            </div>
                            <div class="col-md-2 value">
                                <span><% if(answers.requirements.requirements_basic.c1_req7 === 'true') {%>Yes<%} else {%>No<%} %></span>
                            </div>
                        </div>
                    </div>
                </div>
                <h4 class="text-position">
                    <hr class="rule">
                    Intermediate
                    <hr class="rule">
                </h4>
                <!-- Requirement section Intermediate questions and answers goes here               -->

                <div class="row">
                    <div class="col-md-8 questionsbox">
                        <div class="row">
                            <div class="col-md-10 questions">
                                <p class="data">
                                    Does Acceptance criteria include functional and non-functional test types?
                                </p>
                            </div>
                            <div class="col-md-2 questions">
                                <span><% if(answers.requirements.requirements_intermediate.c2_req1 === 'true') {%>Yes<%} else {%>No<%} %></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-10 questions">
                                <p class="data">
                                    Have at least 75% of Requirements been Risk Assessed for Testing?
                                </p>
                            </div>
                            <div class="col-md-2 questions">
                                <span><% if(answers.requirements.requirements_intermediate.c2_req2 === 'true') {%>Yes<%} else {%>No<%} %></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-10 questions">
                                <p class="data">
                                    Has Test Type Coverage been completed for all requirements?
                                </p>
                            </div>
                            <div class="col-md-2 questions">
                                <span><% if(answers.requirements.requirements_intermediate.c2_req3 === 'true') {%>Yes<%} else {%>No<%} %></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-10 questions">
                                <p class="data">
                                    Has testing in / out of sprints been confirmed for each requirement?
                                </p>
                            </div>
                            <div class="col-md-2 value">
                                <span><% if(answers.requirements.requirements_intermediate.c2_req4 === 'true') {%>Yes<%} else {%>No<%} %></span>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Requirement section Mature questions and answers goes here               -->
                <h4 class="text-position">
                    Mature
                    <hr class="rule">
                </h4>

                <div class="row">
                    <div class="col-md-8 questionsbox">
                        <div class="row">
                            <div class="col-md-10 questions">
                                <p class="data">
                                    Is functional testing planned in sprints more than out of sprint?
                                </p>
                            </div>
                            <div class="col-md-2 questions">
                                <span><% if(answers.requirements.requirements_mature.c3_req1 === 'true') {%>Yes<%} else {%>No<%} %></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-10 questions">
                                <p class="data">
                                    Have all Requirements been Risk Assessed for Testing?
                                </p>
                            </div>
                            <div class="col-md-2 questions">
                                <span><% if(answers.requirements.requirements_mature.c3_req2 === 'true') {%>Yes<%} else {%>No<%} %></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-10 questions">
                                <p class="data">
                                    Is non-functional testing planned in sprints more than out of sprints?
                                </p>
                            </div>
                            <div class="col-md-2 questions">
                                <span><% if(answers.requirements.requirements_mature.c3_req3 === 'true') {%>Yes<%} else {%>No<%} %></span>
                            </div>
                        </div>
                    </div>
                </div>

                <h6 class="questions text-position">
                    Section 2. Testing
                </h6>

                <!-- Testing section Basic questions and answers goes here               -->

